{
  "version": 2,
  "name": "woo-next",
  "alias": "woo-next",
  "builds": [
	{
	  "src": "/static/*",
	  "use": "@now/static"
	},
	{
	  "src": "package.json",
	  "use": "@now/next"
	}
  ],
  "routes": [
	{
	  "src": "/product/(?<slug>[^/]+)$",
	  "dest": "/product?slug=$slug"
	},
	{
	  "src": "/static/(.*)",
	  "dest": "/static/$1"
	}
  ]
}
